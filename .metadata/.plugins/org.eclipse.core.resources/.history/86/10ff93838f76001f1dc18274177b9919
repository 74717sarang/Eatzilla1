package co.string;

public class RemoveChar {

	public static void main(String[] args) {

		String str1="AABBCC";
		String str2="AB";
		
		System.out.println("Remening::"+remove(str1,str2));
		
	}

	private static String remove(String str1, String str2) {
         String str="";
          str=str1.replaceAll("A", "");
          
             
         
		return str;
	}

}

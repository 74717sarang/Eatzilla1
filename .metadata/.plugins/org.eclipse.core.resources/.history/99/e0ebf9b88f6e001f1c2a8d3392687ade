package com.star;

import java.awt.DisplayMode;

public class FloydS_Triangle {
	
	public void dispaly() {
		System.out.println("in dis"); 
	}
	
     
	public static void main(String[] args) {
     /*
      * 1
      * 23
      * 456
      * 78910
      * */
		FloydS_Triangle f = null;
	  f.dispaly();
	  int
	  row=4;
		int num=1;
		for(int i=0;i<row;i++) {
			for(int j=0;j<i+1;j++) {
				System.out.print(num++ +" ");
			}
			System.out.println();
			
		}
		
		
		
	}

}

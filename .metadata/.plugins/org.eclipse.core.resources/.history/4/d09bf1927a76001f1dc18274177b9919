package com.array;

public class SubArrayDivide {

	public static void main(String[] args) {
		
	int []arr=new int[]{2,3,1,5,4};	
     int subLength=3;
	int []sumarr=new int[2];
	
		for(int i=0;i<arr.length;i++) {
			int sum=0;
			for(int j=0;j<i+subLength ;j++) {
				sum+=arr[j];
			}
			sumarr[i]=sum;
			System.out.print(sumarr[i]+" ");
		}
	    	
	}
	
}

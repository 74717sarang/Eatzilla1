package com.star;

public class Trisngle {
	public static void main(String[] args) {

		int row=5;
		int n=row/2;
		for(int i=0;i<=n;i++) {
			
			for(int j=0;j<n-i;j++) {
				System.out.print(" ");
			}
			for(int k=0;k<2*i+1;k++) {
				System.out.print("*");
			}
		//System.out.println();	
		}
	}
}

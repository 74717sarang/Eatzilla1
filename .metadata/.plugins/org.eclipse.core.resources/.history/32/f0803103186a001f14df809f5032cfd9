package com.basic;

public class test {

	public static void main(String[] args) {
		int n=123;
		int num=n;
		int rev = 0;int rem;
		while(num!=0) {
			rem=num%10;
			rev=rev*10+rem;
			num/=10;
			
		}
		System.out.println(rev);
		 
		String str=Integer.toString(num);
	       StringBuilder sb=new StringBuilder(str);
	       sb.reverse();
		  System.out.println(sb.toString());
		
	}
	
	
}

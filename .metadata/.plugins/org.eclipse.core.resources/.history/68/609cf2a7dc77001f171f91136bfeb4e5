package com.basic;

public class PrimeNo {

	
	public static void main(String[] args) {
		
		int num=11;
		boolean prime=true; 
		
		for(int i=2;i<Math.sqrt(11);i++) {
			if(num%i==0) {
				prime=false;
				System.out.println(num+"Not Prime No..."); 

			}
			System.out.println(num+" Prime No..."); 
	
		}
		
		
	} 
	
	
}

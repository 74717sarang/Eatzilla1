package com.array;

public class CommanElement {

	
	public static void main(String[] args) {

		int[] arr1 = { 1, 2, 2, 1 };
		int[] arr2 = { 1, 2 };
      int  n=0;
		int[] arr = new int[n] ;

		for (int i = 0; i < arr2.length; i++) {
			for (int j = 0; j < arr1.length; j++) {
				if (arr2[i] == arr1[j]) {
					arr[i] = arr1[i];
					++n;
				}
			}

		}
		for(int i=0;i<arr.length;i++) {
			
			System.out.println(arr[i]);
		}

	}

}
